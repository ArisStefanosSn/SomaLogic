#!/bin/bash --login

#SBATCH -J METAL
#SBATCH -o METAL.log
#SNATCH -a 1-1381%10
#SBATCH -p long
#SBATCH -t 4-0:0
#SBATCH -D /scratch/jhz22/SomaLogic
#SBATCH --export ALL

export id=$SLURM_ARRAY_TASK_ID

for p in $(awk 'NR==(a-1)*cs+1, NR==a*cs' a=$id cs=10 doc/SomaLogic.list)
do
   echo ${p}
   metal METAL/${p}.metal
   gzip -f METAL/${p}-1.tbl.gz
done
