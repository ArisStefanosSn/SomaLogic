#!/bin/bash --login
# 20-12-2018 JHZ

#SBATCH -J METAL
#SBATCH -o METAL.log
#SNATCH -a 1-139
#SBATCH -p long
#SBATCH -t 90
#SBATCH --export ALL
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=1

cd $HOME/SomaLogic
for p in $(awk 'NR==(a-1)*cs+1, NR==a*cs' a=$SLURM_ARRAY_TASK_ID cs=10 doc/SomaLogic.list)
do
   echo ${p}
   metal METAL/${p}.metal
   gzip -f METAL/${p}-1.tbl.gz
done
