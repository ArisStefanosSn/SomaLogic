#!/bin/bash --login

#SBATCH -J METAL
#SBATCH -o METAL.log
#SNATCH -a 1-13
#SBATCH -p long
#SBATCH -t 4-0:0
#SBATCH -D /scratch/jhz22/SomaLogic
#SBATCH --export ALL

export id=$SLURM_ARRAY_TASK_ID

for p in $(awk 'NR==(a-1)*cs+1, NR==a*cs' a=$id cs=106 doc/SomaLogic.list)
do
   echo ${p}
   if [ $(grep PROCESS METAL/${p}.metal | wc -l) -gt 1 ]; then
      metal METAL/${p}.metal
   fi
   gzip -f METAL/${p}-1.tbl.gz
done
