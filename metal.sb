#!/bin/bash --login
# 20-12-2018 JHZ

#SBATCH -J METAL
#SBATCH -o METAL.log
#SNATCH -a 1-139
#SBATCH -p short
#SBATCH -t 700
#SBATCH --export ALL
#SBATCH --nodes=1
#SBATCH --ntasks-per-node=1
#SBATCH --cpus-per-task=1

export rt=$HOME/SomaLogic
export bs=10

cd $rt
for p in $(awk 'NR==(b-1)*bs+1, NR==b*bs' b=$SLURM_ARRAY_TASK_ID bs=$bs doc/SomaLogic.list)
do
   echo ${p}
   metal METAL/${p}.metal
   gzip -f METAL/${p}-1.tbl.gz
done
